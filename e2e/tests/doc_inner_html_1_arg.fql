LET url = @static + '/simple.html'
LET doc = PAGE(url)

LET expected = `<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8"/>
    <title>Title</title>
</head>
<body>
    <h1>Hello world</h1>
</body>
</html>`
LET actual = INNER_HTML(doc)

LET r1 = '(\s|\")'
LET r2 = '(\n|\s|\")'

RETURN EXPECT(REGEXP_REPLACE(expected, r1, ''), REGEXP_REPLACE(TRIM(actual), r2, ''))