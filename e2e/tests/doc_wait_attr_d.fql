LET url = @dynamic + "?redirect=/events"
LET doc = DOCUMENT(url, true)

WAIT_ELEMENT(doc, "#page-events")

// with fixed timeout
CLICK(doc, "#wait-class-btn")
WAIT_ATTR(doc, "#wait-class-content", "class", "alert alert-success")

// with random timeout
CLICK(doc, "#wait-class-random-btn")
WAIT_ATTR(doc, "#wait-class-random-content", "class", "alert alert-success", 10000)

RETURN ""