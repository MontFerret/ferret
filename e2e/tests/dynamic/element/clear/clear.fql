LET url = @lab.cdn.dynamic + "?redirect=/forms"
LET doc = DOCUMENT(url, true)

WAIT_ELEMENT(doc, "form")

LET input = ELEMENT(doc, "#text_input")

INPUT(input, "Foo", 100)

INPUT_CLEAR(input)

T::EMPTY(INNER_TEXT(doc, "#text_output"))

INPUT(input, "test0-test1", 100)

INPUT_CLEAR(input)

T::EMPTY(INNER_TEXT(doc, "#text_output"))

INPUT(input, "test0&test1", 100)

INPUT_CLEAR(input)

T::EMPTY(INNER_TEXT(doc, "#text_output"))

RETURN NONE