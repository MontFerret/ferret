LET url = @dynamic
LET doc = DOCUMENT(url, true)

LET expected = `<span>Hello</span>`

INNER_HTML_SET(doc, "body", "<span>Hello</span>")

LET actual = INNER_HTML(doc, "body")

LET r1 = '(\s|\")'
LET r2 = '(\n|\s|\")'

RETURN EXPECT(REGEXP_REPLACE(expected, r1, ''), REGEXP_REPLACE(TRIM(actual), r2, ''))