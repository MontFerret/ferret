LET url = @lab.cdn.static + '/simple.html'
LET doc = DOCUMENT(url)

LET expected = `<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8"/>
    <title>Title</title>
</head>
<body>
    <h1>Hello world</h1>
</body>
</html>`
LET actual = INNER_HTML(doc)

LET r1 = '(\s|\")'
LET r2 = '(\n|\s|\")'

RETURN T::EQ(REGEX_REPLACE(TRIM(actual), r2, ''), REGEX_REPLACE(expected, r1, ''))